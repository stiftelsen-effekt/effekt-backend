{
  "name": "stiftelsen-effekt-backend",
  "version": "0.0.1",
  "description": "Backend for stiftelsen gi effektivt sitt donasjonsystem",
  "engines": {
    "node": "^16.0.0"
  },
  "scripts": {
    "test": "mocha --require ts-node/register --require dotenv/config",
    "test-coverage": "nyc --reporter=text mocha --require ts-node/register src/__test__",
    "typecheck": "tsc --noEmit",
    "build": "copyfiles -u 1 src/views/**/* src/views/**/**/* src/views/**/**/**/* dist/ && tsc",
    "start": "node ./dist/server.js",
    "dev": "export NODEMON=true && nodemon src/server.ts",
    "debug": "export NODEMON=true && nodemon --exec 'node --inspect=0.0.0.0:9229 --require ts-node/register src/server.ts'",
    "generate-types-from-schema": "cd src && npx openapi-typescript \"specs/**/*.yaml\" --output schemas/",
    "format": "prettier --write \"./**/*{js,ts,jsx,tsx,json,md,css,scss,yml,yaml}\"",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fellmirr/effekt-backend.git"
  },
  "author": "HÃ¥kon Harnes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fellmirr/effekt-backend/issues"
  },
  "homepage": "https://github.com/fellmirr/effekt-backend#readme",
