    "decimal.js": "^7.5.1",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-fileupload": "^1.2.0",
    "express-oauth2-jwt-bearer": "^1.1.0",
    "express-prettify": "0.0.7",
    "express-rate-limit": "^2.14.2",
    "fs-extra": "^4.0.3",
    "graceful-fs": "^4.2.4",
